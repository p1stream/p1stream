    },
    "main": "lib/index.js",
    "bin": {
        "p1stream": "main.js"
    },
    "dependencies": {
        "clone": "0.2",
        "node-uuid": "1",
        "express": "4",
        "underscore": "1",
        "symmetry": "0.4",
        "write-ebml": "p1stream/write-ebml"
    },
    "devDependencies": {
        "request": "2"
    },
    "devToolDependencies": {
        "p1-build": "p1stream/p1-build",
