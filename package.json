        "bower": "1"
    },
    "pluginDependencies": {
        "darwin": {
            "p1-mac-sources": "p1stream/p1-mac-sources"
        }
    },
    "jshintConfig": {
        "enforceall": true,
        "bitwise": false,
        "curly": false,
        "singleGroups": false,
        "es3": false,
        "node": true,
        "browser": true,
        "predef": ["angular", "EventSource", "Symmetry"]
