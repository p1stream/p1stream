{
    "name": "p1stream",
    "version": "0.0.0",
    "description": "Live streaming tool",
    "private": true,
    "author": "St√©phan Kochen <stephan@kochen.nl>",
    "repository": {
        "type": "git",
        "url": "http://github.com/p1stream/p1stream.git"
    },
    "main": "lib/index.js",
    "bin": {
        "p1stream": "p1stream.js"
    },
    "dependencies": {
        "clone": "0.2",
        "express": "4",
        "lodash": "4",
        "symmetry": "0.4",
        "jmsg": "0.1",
        "chalk": "1",
        "bunyan": "1",
        "humanize-duration": "2",
        "write-ebml": "p1stream/write-ebml"
    },
    "devToolDependencies": {
        "p1-build": "p1stream/p1-build",
        "node-gyp": "1",
        "bower": "1"
    },
    "pluginDependencies": {
        "darwin": {
            "p1-mac-plugins": "p1stream/p1-mac-plugins",
            "p1-rtmp-plugins": "p1stream/p1-rtmp-plugins"
        }
    },
    "jshintConfig": {
        "enforceall": true,
        "bitwise": false,
        "curly": false,
        "singleGroups": false,
        "esnext": true,
        "node": true,
        "browser": true,
        "predef": ["angular", "EventSource", "Symmetry"]
    }
}
